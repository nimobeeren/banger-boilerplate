{
  "name": "banger-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n next,generate 'next dev' 'yarn:watch:*'",
    "generate": "yarn generate:prisma && yarn generate:schema && yarn generate:web",
    "generate:prisma": "prisma generate",
    "generate:schema": "cross-env GENERATE=true ts-node --skip-project graphql/schema/index.ts",
    "generate:web": "graphql-codegen",
    "watch": "concurrently 'yarn:watch:*'",
    "watch:prisma": "yarn generate:prisma --watch",
    "watch:schema": "nodemon -w graphql/schema -e ts -x 'yarn generate:schema'",
    "watch:web": "yarn generate:web --watch",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext js,ts,tsx"
  },
  "dependencies": {
    "@graphql-codegen/add": "^1.15.0",
    "@graphql-codegen/cli": "^1.15.0",
    "@graphql-codegen/typescript": "^1.15.0",
    "@graphql-codegen/typescript-operations": "^1.15.0",
    "@graphql-codegen/typescript-react-apollo": "^1.15.0",
    "@nexus/schema": "^0.13.1",
    "@prisma/cli": "^2.0.0-beta.6",
    "@prisma/client": "^2.0.0-beta.6",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@typescript-eslint/eslint-plugin": "^2",
    "@typescript-eslint/parser": "^2",
    "apollo-boost": "^0.4.9",
    "apollo-cache": "^1.3.5",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-server-micro": "^2.14.0",
    "apollo-utilities": "^1.3.4",
    "babel-eslint": "^10",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.x",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^2",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.4.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  }
}
