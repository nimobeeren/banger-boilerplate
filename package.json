{
  "name": "banger-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n next,generate 'next dev' 'yarn:generate:watch'",
    "build": "next build",
    "start": "next start",
    "generate": "yarn generate:schema && yarn generate:web",
    "generate:schema": "ts-node --skip-project graphql/schema/index.ts",
    "generate:web": "graphql-codegen",
    "generate:watch": "concurrently 'yarn:generate:watch:*'",
    "generate:watch:schema": "nodemon -w graphql/schema -e ts -x 'yarn generate:schema'",
    "generate:watch:web": "yarn generate:web --watch",
    "lint": "eslint . --ext js,ts,tsx"
  },
  "dependencies": {
    "@graphql-codegen/add": "^1.13.4",
    "@graphql-codegen/cli": "^1.13.4",
    "@graphql-codegen/typescript": "^1.13.4",
    "@graphql-codegen/typescript-operations": "^1.13.4",
    "@graphql-codegen/typescript-react-apollo": "^1.13.4",
    "@nexus/schema": "^0.13.1",
    "@prisma/cli": "^2.0.0-beta.4",
    "@prisma/client": "^2.0.0-beta.4",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "apollo-boost": "^0.4.7",
    "apollo-cache": "^1.3.4",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.14",
    "apollo-server-express": "^2.12.0",
    "apollo-utilities": "^1.3.3",
    "babel-eslint": "10.x",
    "concurrently": "^5.2.0",
    "eslint": "6.x",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "next": "9.3.6",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "react": "16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "16.13.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  }
}
